FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      bioBed.bedT_char;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 338;

boundaryField
{
    top
    {
        type            calculated;
        value           uniform 338;
    }
    bottom
    {
        type            calculated;
        value           uniform 338;
    }
    walls
    {
        type            calculated;
        value           uniform 338;
    }
    frontAndBack
    {
        type            symmetry;
    }
}


