FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      bioBed.bedCp_char;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 -1 0 0 0];

internalField   uniform 1100;

boundaryField
{
    top
    {
        type            calculated;
        value           uniform 1100;
    }
    bottom
    {
        type            calculated;
        value           uniform 1100;
    }
    walls
    {
        type            calculated;
        value           uniform 1100;
    }
    frontAndBack
    {
        type            symmetry;
    }
}


