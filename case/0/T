FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    bottom
    {
        type            fixedValue;
        value           uniform 298;
    }
    top
    {
        type            inletOutlet;
        inletValue      uniform 298;
        value           uniform 298;
    }
    frontAndBack
    {
        type            symmetry;
    }
}


